a = await response.json()\n\n      let city = {}\n\n      city = [\n        { weather: data.weather[0].main },\n        { temp: data.main.temp },\n        { feelsLike: data.main.feels_like },\n        { pressure: data.main.pressure },\n        { tempMax: data.main.temp_max },\n        { tempMin: data.main.temp_min }\n      ]\n\n      this.data = city\n\n      console.log(city)\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n$input-color:rgb(60, 45, 48);\n\n.title {\n  text-transform: uppercase;\n  font-size: 50px;\n}\n\ninput {\n  width: 300px;\n  background: transparent;\n  outline: 0;\n  border: 0;\n  text-transform: uppercase;\n  font-size: 40px;\n  font-weight: 700;\n  font-family: 'Times New Roman', Times, serif;\n  color: $input-color;\n}\n\ninput::placeholder {\n  font-weight: 400;\n  opacity: 1;\n  color: $input-color;\n}\n\n.input {\n  margin-top: 20px;\n  display: flex;\n  text-align: end;\n  align-items: center;\n  gap: 5px;\n  line-height: 15px;\n  font-weight: 500;\n  justify-content: center;\n  color: $input-color;\n\n  p {\n    font-size: 30px;\n  }\n\n  button {\n    background: transparent;\n    border: 1px solid black;\n    outline: 0;\n    padding: 20px;\n    cursor: pointer;\n\n    &:hover {\n      transform: scale(1.05);\n    }\n  }\n}\n</style>"],"mappings":"AAcA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,GAAG,EAAE,kCAAkC;MACvCC,OAAO,EAAE,EAAE;MACXH,IAAI,EAAE;IACR;EACF,CAAC;EACDI,OAAO,EAAE;IACP;IACA,MAAMC,OAAOA,CAAA,EAAG;MACd,IAAIC,QAAO,GAAI,IAAI,CAACL,IAAI,CAACM,OAAO,CAAC,GAAG,EAAE,GAAG;MAEzC,IAAIC,QAAO,GAAI,MAAMC,KAAK,CAAE,qDAAoDH,QAAS,UAAS,IAAI,CAACJ,GAAI,EAAC;MAC5G,IAAIF,IAAG,GAAI,MAAMQ,QAAQ,CAACE,IAAI,CAAC;MAE/B,IAAIT,IAAG,GAAI,CAAC;MAEZA,IAAG,GAAI,CACL;QAAEE,OAAO,EAAEH,IAAI,CAACG,OAAO,CAAC,CAAC,CAAC,CAACQ;MAAK,CAAC,EACjC;QAAEC,IAAI,EAAEZ,IAAI,CAACW,IAAI,CAACC;MAAK,CAAC,EACxB;QAAEC,SAAS,EAAEb,IAAI,CAACW,IAAI,CAACG;MAAW,CAAC,EACnC;QAAEC,QAAQ,EAAEf,IAAI,CAACW,IAAI,CAACI;MAAS,CAAC,EAChC;QAAEC,OAAO,EAAEhB,IAAI,CAACW,IA